{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "react",
    "outDir": ".",
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "allowJs": false,
    "sourceMap": false,
    "noImplicitReturns": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "experimentalDecorators": true,
    "watch": true,
    "noImplicitUseStrict": true
  },
  "filesGlob": [
    "*.ts"
  ],
  "types": [
  ],
  "exclude": [
  ],
  "compileOnSave": false
}